{% extends 'base.html.twig' %}

{% block title %}Hello DataImportController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Utilisateurs importés ✅</h1>

    <ul>
        {% for cle, valeur in createdUsers %}
            <li>L'utilisateur : {{ cle }} créé avec le mot de passe {{ valeur }} !</li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
